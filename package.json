{
  "name": "varbintech",
  "version": "1.0.0",
  "description": "A homepage for Varbintech.com",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "dev": "run-s clean eleventy:dev --print-label",
    "build": "run-s clean eleventy:prod --print-label",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
    "clean": "del-cli dist",
    "lint": "eslint src/**/*.js src/assets/**/*.js utils/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js src/assets/**/*.js utils/**/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Varbintech/varbintech"
  },
  "author": "Vitalii Rybka, @var_bincom",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "1.0.1",
    "@11ty/eleventy-img": "2.0.0",
    "@11ty/eleventy-navigation": "0.3.3",
    "@11ty/eleventy-plugin-rss": "1.1.2",
    "@babel/core": "7.13.14",
    "@babel/plugin-transform-runtime": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "babel-loader": "8.2.5",
    "clean-css": "5.3.0",
    "critical": "4.0.1",
    "cssesc": "^3.0.0",
    "del-cli": "^3.0.1",
    "eleventy-plugin-svg-sprite": "1.2.2",
    "focus-trap": "6.9.1",
    "focus-visible": "5.2.0",
    "html-minifier": "4.0.0",
    "luxon": "2.3.2",
    "markdown-it": "13.0.1",
    "memfs": "3.4.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "4.1.5",
    "svg-sprite": "1.5.4",
    "webpack": "5.28.0"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2"
  }
}
